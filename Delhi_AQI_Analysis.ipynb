import pandas as pd
import matplotlib.pyplot as plt

# Sample AQI data
data = {
    'Season': ['Winter', 'Summer', 'Monsoon', 'Post-Monsoon'],
    'AQI': [320, 180, 140, 260]
}

df = pd.DataFrame(data)

# Seasonal AQI Bar Chart
plt.figure()
plt.bar(df['Season'], df['AQI'])
plt.title("Seasonal AQI Variation in Delhi")
plt.xlabel("Season")
plt.ylabel("Average AQI")
plt.show()
